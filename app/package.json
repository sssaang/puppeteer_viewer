{
  "name": "api",
  "description": "api for typed-app",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js ",
    "start:debug": "nodemon -L --inspect-brk=0.0.0.0:9229 --exec babel-node src/index.js",
    "logs": "firebase functions:log",
    "clean-build": "rm -rf dist",
    "build": "npm run build:production",
    "build:production": "npm run clean-build && TARGET_ENV=production babel src --source-maps=true --out-dir dist",
    "build:staging": "npm run clean-build && TARGET_ENV=staging babel src --source-maps=true --out-dir dist",
    "build:dev": "npm run clean-build && TARGET_ENV=dev babel src --source-maps=true --out-dir dist",
    "build:local": "npm run clean-build && TARGET_ENV=local babel src --source-maps=true --out-dir dist",
    "prewatch": "npm run clean-build && firebase functions:config:get -P dev > .runtimeconfig.json",
    "watch": "babel src --watch --source-maps=true --out-dir dist"
  },
  "engines": {
    "node": "12"
  },
  "main": "dist/index.js",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.14.8",
    "@mozilla/readability": "^0.4.1",
    "@sentry/integrations": "^6.2.5",
    "@sentry/node": "^6.0.2",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^9.5.0",
    "firebase-functions": "^3.6.1",
    "fluture": "^13.0.1",
    "fluture-sanctuary-types": "^7.0.0",
    "generic-pool": "^3.8.2",
    "googleapis": "^61.0.0",
    "iconv-lite": "^0.6.2",
    "jsdom": "^17.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "npm": "^6.14.8",
    "open-graph-scraper": "^4.7.1",
    "puppeteer": "^10.2.0",
    "sanctuary": "^3.1.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@firebase/testing": "^0.20.11",
    "@neutrinojs/jest": "^9.5.0",
    "@neutrinojs/standardjs": "^9.5.0",
    "babel-plugin-root-import": "^6.6.0",
    "babel-plugin-source-map-support": "^2.1.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "core-js": "^3.8.3",
    "firebase-functions-test": "^0.2.3",
    "firebase-tools": "^9.6.1",
    "jest": "^26.6.2",
    "neutrino": "^9.5.0",
    "nodemon": "^2.0.12",
    "regenerator-runtime": "^0.13.7",
    "supertest": "^6.1.3"
  },
  "private": true
}
